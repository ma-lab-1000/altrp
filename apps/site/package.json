    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "country-flag-icons": "^1.5.20",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.22",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.22",
    "next": "^15.0.0",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "qs": "^6.14.0",
    "radix-ui": "^1.4.3",
    "react": "^19.1.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.63.0",
    "react-phone-number-input": "^3.4.13",
