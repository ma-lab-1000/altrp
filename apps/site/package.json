{
  "name": "altrp-site",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3100",
    "dev:wrangler": "wrangler pages dev --port 3300",
    "dev:all": "cross-env NEXT_PORT=3100 WRANGLER_PORT=3300 PROXY_PORT=3400 node scripts/dev-server.mjs",
    "build": "next build --experimental-build-mode compile",
    "build:static": "cross-env STATIC_EXPORT=true next build",
    "start": "next start -p 3100",
    "type-check": "tsc --noEmit",
    "d1:create": "wrangler d1 create altrp-site-db",
    "d1:migrate:local": "wrangler d1 execute altrp-site-db --local --file=../../migrations/site/app-schema.sql",
    "d1:migrate:remote": "wrangler d1 execute altrp-site-db --remote --file=../../migrations/site/app-schema.sql",
    "d1:migrate:users:local": "wrangler d1 execute altrp-site-db --local --file=../../migrations/site/users-table.sql",
    "d1:migrate:users:remote": "wrangler d1 execute altrp-site-db --remote --file=../../migrations/site/users-table.sql",
    "d1:query:local": "wrangler d1 execute altrp-site-db --local",
    "d1:query:remote": "wrangler d1 execute altrp-site-db --remote",
    "hash-password": "node scripts/hash-password.mjs",
    "test:int": "bun test --config ./tsconfig.json test/int/",
    "migrate:postgres": "node scripts/migrate-postgres.mjs"
  },
  "dependencies": {
    "@cloudflare/vitest-pool-workers": "^0.10.5",
    "drizzle-orm": "^0.36.4",
    "pg": "^8.13.1",
    "postgres": "^3.4.5",
    "@dnd-kit/modifiers": "^9.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@tabler/icons-react": "^3.35.0",
    "@tailwindcss/postcss": "4.1.13",
    "@tanstack/react-table": "^8.21.3",
